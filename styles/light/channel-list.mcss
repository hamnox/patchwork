ChannelList {
  a.channel {
    background-color: rgba(255, 255, 255, 0.66)
    color: #333
    transition: background-color 0.2s
    -more {
      color: #658aaf
      background: rgb(255, 255, 255)
    }
    :hover {
      background-color: rgba(255, 255, 255, 0.4)
    }
    a {
      font-weight: bold
      -subscribe {
        :hover {
          background-color: rgb(112, 184, 212)
        }
      }
    }
  }
}
