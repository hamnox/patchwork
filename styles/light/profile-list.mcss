ProfileList {
  a.profile {
    background-color: rgba(255, 255, 255, 0.74)
    transition: background-color 0.2s
    @svg connected {
      circle {
        fill: green
      }
    }
    div.main {
      div.name {
        font-weight: bold
        font-size: 110%
        color: #636363
      }
    }
    :hover {
      background-color: rgba(255, 255, 255, 0.4)
    }
  }
}
