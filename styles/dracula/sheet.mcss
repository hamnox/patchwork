Sheet {
  background: #343744
  box-shadow: 0 0 40px black
  border-bottom-left-radius: 3px
  border-bottom-right-radius: 3px
  -private {
    footer {
      border-color: #5b56b3
      background-color: #50fa7ba56
    }
  }
  -public {
    footer {
      background-color: #6f482c
    }
  }
  footer {
    border-top: 1px solid #6272a4
    button {
      border: 1px solid #44475a
    }
  }
}
